<body>
  <div id="map" style="width: 100%; height: 100%;""></div>
  <script>
    var map = L.map('map', {
      zoomControl: false,
      doubleTapDragZoom: 'center',
      doubleTapDragZoomOptions: {
        reverse: true,
      }
    }).setView([29.762778, -95.383056], 12);
    L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
      maxZoom: 19,
      attribution: '&copy; <a href="http://www.openstreetmap.org/copyright" target="_new">OpenStreetMap</a> | <a href="https://github.com/c360e5f1/houstonincidents.com" target=_new">GitHub</a> | poots'
    }).addTo(map);
    map.locate({setView: true, maxZoom: 8});
